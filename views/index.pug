html
  head
    meta(charset= 'UTF-8')
    meta(
      name= 'viewport'
      content= 'width=device-width, initial-scale=1.0'
      )
    title= title
  body
    h1= headerOne
    ul
      each message in messages
        li
          h2= message.text
          div= message.user
          div= message.added